{
  "name": "브랜드 매칭 시스템",
  "image": "mcr.microsoft.com/vscode/devcontainers/python:3.9",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.9"
    }
  },
  "postCreateCommand": "pip install -r requirements.txt",
  "forwardPorts": [5002],
  "portsAttributes": {
    "5002": {
      "label": "브랜드 매칭 웹앱",
      "onAutoForward": "openPreview"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.flake8"
      ]
    }
  }
} 